<h2>配送先編集</h2>

<%= form_with model: @address, url: public_shipping_address_path(@address) do |f| %>
  <%= f.label :郵便番号（ハイフンなし） %>
  <%= f.text_field :postal_code %>
  
  <%= f.label :住所 %>
  <%= f.text_field :address %>
  
  <%= f.label :宛名 %>
  <%= f.text_field :name %>
  
  <%= f.submit '変更を保存' %>
<% end %>